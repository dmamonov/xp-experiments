import util.cmd.Console;

public class Each<T> {
  public T[] $values;

  protected __construct(T[] $values) {
    $this->values= $values;
  }
  
  public static void main(string[] $args) {
    foreach ($value in new self<string>($args)->values) {
      Console::writeLine('- ', $value);
    }
    for ($i= 0; $i < 10; $i++) {
      if ($i > 5) continue;
      Console::writeLine('- ', $i);
    }
  }  
}
